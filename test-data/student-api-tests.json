{
  "test_cases": [
    {
      "name": "Test Case 1: Valid Student Creation",
      "endpoint": "POST http://localhost:3000/students",
      "headers": {
        "Content-Type": "application/json"
      },
      "request_body": {
        "firstName": "John",
        "lastName": "Doe",
        "dateOfBirth": "2005-06-15",
        "contactNumber": "1234567890",
        "email": "john.doe@example.com",
        "guardianName": "Jane Doe",
        "guardianContact": "9876543210",
        "username": "john_doe",
        "password": "securePassword123"
      },
      "expected_response": {
        "status": 201,
        "body": {
          "studentId": "number",
          "firstName": "John",
          "lastName": "Doe",
          "dateOfBirth": "2005-06-15T00:00:00.000Z",
          "contactNumber": "1234567890",
          "email": "john.doe@example.com",
          "guardianName": "Jane Doe",
          "guardianContact": "9876543210",
          "admissionDate": "2025-12-08T00:00:00.000Z",
          "status": "ACTIVE",
          "username": "john_doe",
          "message": "Student added successfully"
        }
      }
    },
    {
      "name": "Test Case 2: Student with Optional Fields Empty",
      "endpoint": "POST http://localhost:3000/students",
      "headers": {
        "Content-Type": "application/json"
      },
      "request_body": {
        "firstName": "Jane",
        "lastName": "Smith",
        "contactNumber": "9876543210",
        "guardianName": "John Smith",
        "guardianContact": "5555555555",
        "username": "jane_smith",
        "password": "securePassword456"
      },
      "expected_response": {
        "status": 201,
        "body": {
          "studentId": "number",
          "firstName": "Jane",
          "lastName": "Smith",
          "dateOfBirth": null,
          "contactNumber": "9876543210",
          "email": null,
          "guardianName": "John Smith",
          "guardianContact": "5555555555",
          "admissionDate": "2025-12-08T00:00:00.000Z",
          "status": "ACTIVE",
          "username": "jane_smith",
          "message": "Student added successfully"
        }
      }
    },
    {
      "name": "Test Case 3: Missing Required Field (firstName)",
      "endpoint": "POST http://localhost:3000/students",
      "headers": {
        "Content-Type": "application/json"
      },
      "request_body": {
        "lastName": "Doe",
        "contactNumber": "1111111111",
        "guardianName": "Parent",
        "guardianContact": "2222222222",
        "username": "test_user",
        "password": "password123"
      },
      "expected_response": {
        "status": 400,
        "body": {
          "message": "firstName should not be empty",
          "error": "Bad Request"
        }
      }
    },
    {
      "name": "Test Case 4: Invalid Email Format",
      "endpoint": "POST http://localhost:3000/students",
      "headers": {
        "Content-Type": "application/json"
      },
      "request_body": {
        "firstName": "Bob",
        "lastName": "Johnson",
        "email": "invalid-email",
        "contactNumber": "3333333333",
        "guardianName": "Parent",
        "guardianContact": "4444444444",
        "username": "bob_johnson",
        "password": "password789"
      },
      "expected_response": {
        "status": 400,
        "body": {
          "message": "email must be an email",
          "error": "Bad Request"
        }
      }
    },
    {
      "name": "Test Case 5: Invalid Date Format",
      "endpoint": "POST http://localhost:3000/students",
      "headers": {
        "Content-Type": "application/json"
      },
      "request_body": {
        "firstName": "Alice",
        "lastName": "Williams",
        "dateOfBirth": "06-15-2005",
        "contactNumber": "5555555555",
        "guardianName": "Parent",
        "guardianContact": "6666666666",
        "username": "alice_williams",
        "password": "password999"
      },
      "expected_response": {
        "status": 400,
        "body": {
          "message": "dateOfBirth must be a valid ISO 8601 date string",
          "error": "Bad Request"
        }
      }
    }
  ],
  "schema_mapping": {
    "Student Model Fields": {
      "studentId": "Auto-generated (Int) @id",
      "firstName": "String (Required) - Maps to 'first_name' in DB",
      "lastName": "String (Required) - Maps to 'last_name' in DB",
      "dateOfBirth": "DateTime? (Optional) - Maps to 'date_of_birth' in DB",
      "contactNumber": "String (Unique, Required) - Maps to 'contact_number' in DB",
      "email": "String? (Optional, Unique) - Direct field",
      "guardianName": "String (Required) - Maps to 'guardian_name' in DB",
      "guardianContact": "String (Required) - Maps to 'guardian_contact' in DB",
      "admissionDate": "DateTime (Default: now()) - Maps to 'admission_date' in DB",
      "status": "String (Default: 'ACTIVE') - Options: ACTIVE, INACTIVE, LEFT"
    },
    "User Model Fields": {
      "userId": "Auto-generated (Int) @id - Maps to 'user_id' in DB",
      "username": "String (Unique, Required) - Direct field",
      "passwordHash": "String (Required) - Maps to 'password_hash' in DB",
      "role": "String (Default: 'STUDENT') - Options: admin, STUDENT",
      "studentId": "Foreign Key - Links to Student"
    }
  },
  "field_validation_rules": {
    "firstName": "String, Required, Not Empty",
    "lastName": "String, Required, Not Empty",
    "dateOfBirth": "Optional, ISO 8601 Format (YYYY-MM-DD)",
    "contactNumber": "String, Required, Not Empty, Unique in Database",
    "email": "Optional, Valid Email Format, Unique in Database",
    "guardianName": "String, Required, Not Empty",
    "guardianContact": "String, Required, Not Empty",
    "username": "String, Required, Not Empty, Unique in Database",
    "password": "String, Required, Not Empty (Will be hashed with bcrypt)"
  },
  "powershell_curl_command": "curl -Method POST -Uri http://localhost:3000/students -Headers @{ \"Content-Type\" = \"application/json\" } -Body '{\"firstName\": \"John\", \"lastName\": \"Doe\", \"dateOfBirth\": \"2005-06-15\", \"contactNumber\": \"1234567890\", \"email\": \"john.doe@example.com\", \"guardianName\": \"Jane Doe\", \"guardianContact\": \"9876543210\", \"username\": \"john_doe\", \"password\": \"securePassword123\"}'"
}
